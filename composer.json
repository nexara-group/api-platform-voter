{
  "name": "nexara/api-platform-voter",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/security-bundle": "^6.4 || ^7.0",
    "symfony/cache": "^6.4 || ^7.0",
    "psr/cache": "^3.0",
    "api-platform/core": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Nexara\\ApiPlatformVoter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nexara\\ApiPlatformVoter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "bundle-class": "Nexara\\ApiPlatformVoter\\NexaraApiPlatformVoterBundle"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symplify/easy-coding-standard": "^12.3",
    "rector/rector": "^1.2"
  }
}
